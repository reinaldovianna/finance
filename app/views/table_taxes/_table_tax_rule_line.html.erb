<tr class="table-tax-rule" >
  <td>
    <%= rule.rows.map(&:row_name).join(' + ') %>
  </td>
  <td>
    <%= rule.operator %>
  </td>
  <td>
    <%= rule.measurement %>
  </td>
  <td>
    <%= rule.value %>
  </td>
  <td>
    <%= link_to(remove_rule_path, 'data-table-tax-rule-id' => rule.id.to_s, class:"btn btn-danger remove-table-tax-rule") do %>
      <i class="fa fa-trash-o" aria-hidden="true"></i>
    <% end %>
  </td>
</tr>